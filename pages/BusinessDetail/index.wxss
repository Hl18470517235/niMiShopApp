#detailbox{
  position: fixed;
  z-index: 999999;
  transition: all 1s;
  bottom: 0rpx;
  padding-top: 70rpx;
  left: 0rpx;
  right: 0rpx;
  background-color: #fff;
  border-radius: 30rpx 30rpx 0 0;
  overflow: hidden;
  animation: Box 0.7s;
}   
@keyframes Box
{
  0%   {bottom: -50%;}
	100%  {bottom: 0rpx;}
}
