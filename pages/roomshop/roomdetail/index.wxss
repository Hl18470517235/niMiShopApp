#actionStyle{
  position: absolute;
  z-index: 889;
  bottom: 65rpx;
  right: 120rpx;
  box-sizing: border-box;
  padding:6rpx;
  display: flex;
  justify-content: space-around;
  height: 30rpx;
  width: 30rpx;
  border-radius: 50%;
  background-color: #FF7875;
  transform: rotate(180deg);
  overflow: hidden;
}
#actionStyle .one {
  height: 20rpx;
  width: 18%;
  background-color: #FFF;
  /* animation:myfirst 2s;
  animation-direction:alternate; */
  animation: myfirst 1s linear infinite;
}
@keyframes myfirst
{
  0%   {height: 0rpx;}
  25%   {height: 24rpx;}
  50%   {height: 12rpx;}
	100%  {height:32rpx}
}
#actionStyle .two {
  height: 32rpx;
  width: 18%;
  background-color: #FFF;
  animation: myfirsttwo 1.5s linear infinite;
}
@keyframes myfirsttwo
{
  0%   {height: 10rpx;}
  25%   {height: 30rpx;}
  50%   {height: 20rpx;}
	100%  {height:24rpx}
}
#actionStyle .three {
  height: 16rpx;
  width: 18%;
  background-color: #FFF;
  animation: myfirstthree 1s linear infinite;
}
@keyframes myfirstthree
{
  0%   {height: 10rpx;}
  25%   {height: 6rpxrpx;}
  50%   {height: 12rpx;}
	100%  {height:16rpx}
}