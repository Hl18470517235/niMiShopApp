/* pages/roomshop/roomdetail/index.wxss */
.header {
  position: relative;
  display: flex;
  align-items: center;
  height: 184rpx;
  background-color: #FF2F56;
}
.header .img {
  height: 120rpx;
  width: 120rpx;
  border-radius: 50%;
  margin-left: 40rpx;
}
.header .title {
  margin-left: 40rpx;
}
.titlebottom {
  position: relative;
  height: 160rpx;
  background-color: #FF2F56;
  border-radius: 0 0 30rpx 30rpx;
}
.detailList {
  position: relative;
  box-sizing: border-box;
  margin: 16rpx;
  margin-top: 0rpx;
  background-color: #FFF;
  border-radius: 20rpx;
  padding:20rpx;
  box-shadow: 0px 2px 7px 2px rgba(0, 0, 0, 0.09);
}
.historyList {
