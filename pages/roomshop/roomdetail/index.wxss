  display: inline-block;
  margin-left: 30rpx;
  font-size: 20rpx;
  align-content: space-between;
}
.listBottom .imgbox {
  display: flex;
}
.detailList .imgbox .roomimg {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.imgbox .img-box {
  position: relative;
  width: 456rpx;
