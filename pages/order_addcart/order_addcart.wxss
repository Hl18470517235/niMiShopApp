.shoppingCart{width:712rpx;margin:0 auto;}
.shoppingCart .labelNav{height:76rpx;padding:0 30rpx;font-size:22rpx;color:#8c8c8c;position:fixed;left:0;width:100%;box-sizing:border-box;background-color:#f5f5f5;z-index:5;}
.shoppingCart .labelNav .item .iconfont{font-size:25rpx;margin-right:10rpx;}
.shoppingCart .nav{width:100%;height:80rpx;background-color:#fff;padding:0 30rpx;box-sizing:border-box;font-size:28rpx;color:#282828;position:fixed;left:0;z-index:5;}
.shoppingCart .nav .administrate{font-size:26rpx;color:#282828;width:110rpx;height:46rpx;border-radius:6rpx;border:1px solid #868686;}
.shoppingCart .noCart{margin-top:171rpx;padding-top: 0.1rpx;}
.shoppingCart .noCart .pictrue{width:480rpx;height:480rpx;margin:78rpx auto 0rpx auto;}
.shoppingCart .noCart .pictrue image{width:100%;height:100%;}
.shoppingCart .store{border-radius: 20rpx; box-shadow: 0 4rpx 12rpx 0 rgba(0, 0, 0, 0.4);overflow: hidden;margin-top:16rpx;background-color: white;}
.shoppingCart .store .title{padding: 23rpx 16rpx;}
.shoppingCart .store .title .name{margin-left:5rpx;width:600rpx;display: inline-block;}
.shoppingCart .list{margin-top:0;}
.shoppingCart .list .item{padding:22rpx 16rpx 46rpx;border-top:2rpx solid #F5F5F5;}
.shoppingCart .list .item .picTxt{width:627rpx;position:relative;}
.shoppingCart .list .item .picTxt .pictrue{width:140rpx;height:140rpx;}
.shoppingCart .list .item .picTxt .pictrue image{width:100%;height:100%;border-radius:10rpx;}
.shoppingCart .list .item .picTxt .text{width:464rpx;min-height: 140rpx; font-size:28rpx;color:#101010;}
.shoppingCart .list .item .picTxt .text .infor{font-size:24rpx;color:#868686;}
.shoppingCart .list .item .picTxt .text .money{color:#DF5E39;display:inline;font-weight: bold;}
.shoppingCart .list .item .picTxt .text .unit{color:#D0d0d0;display: inline;}
.shoppingCart .list .item .picTxt .text .line2 {padding: 10rpx 0;font-size:20rpx;}
.shoppingCart .list .item .picTxt .text .line2 .in{border-radius: 20rpx;display: inline-block; padding:2rpx 10rpx;margin-right:10rpx;}
.shoppingCart .list .item .picTxt .text .line2 .ps{background-color:rgba(67,219,203, 0.3);color:#006D75;}
.shoppingCart .list .item .picTxt .text .line2 .zy{color:#FF4D4F;background-color: rgba(255,120,117,0.3);}
.shoppingCart .list .item .picTxt .text .line3 {position: relative;}
.shoppingCart .list .item .picTxt .carnum{height:44rpx;position:absolute;bottom:7rpx;right:30rpx;}
.shoppingCart .list .item .picTxt .carnum view{border:1rpx solid #a4a4a4;text-align:center;height:44rpx;line-height:40rpx;font-size:28rpx;color:#D0d0d0;}
.shoppingCart .list .item .picTxt .carnum .reduce{border-right:0;border-radius:3rpx 0 0 3rpx;width:44rpx;height:44rpx;}
.shoppingCart .list .item .carnum .image{width:50%;height:50%;}
.shoppingCart .list .item .picTxt .carnum .reduce.on{border-color:#e3e3e3;color:#dedede;}
.shoppingCart .list .item .picTxt .carnum .plus{border-left:0;border-radius:0 3rpx 3rpx 0;width:44rpx;height: 44rpx;}
.shoppingCart .list .item .picTxt .carnum .num{color:#101010;width:56rpx;}
.shoppingCart .invalidGoods{background-color:#fff;}
.shoppingCart .invalidGoods .goodsNav{width:100%;height:66rpx;padding:0 30rpx;box-sizing:border-box;font-size:28rpx;color:#282828;}
.shoppingCart .invalidGoods .goodsNav .iconfont{color:#424242;font-size:28rpx;margin-right:17rpx;}
.shoppingCart .invalidGoods .goodsNav .del{font-size:26rpx;color:#999;}
.shoppingCart .invalidGoods .goodsNav .del .icon-shanchu1{color:#999;font-size:33rpx;vertical-align:-2rpx;margin-right:8rpx;}
.shoppingCart .invalidGoods .goodsList .item{padding:20rpx 30rpx;border-top:1rpx solid #f5f5f5;}
.shoppingCart .invalidGoods .goodsList .item .invalid{font-size:22rpx;color:#fff;width:70rpx;height:36rpx;background-color:#aaa;border-radius:3rpx;text-align:center;line-height:36rpx;}
.shoppingCart .invalidGoods .goodsList .item .pictrue{width:140rpx;height:140rpx;}
.shoppingCart .invalidGoods .goodsList .item .pictrue image{width:100%;height:100%;border-radius:6rpx;}
.shoppingCart .invalidGoods .goodsList .item .text{width:433rpx;font-size:28rpx;color:#999;height:140rpx;}
.shoppingCart .invalidGoods .goodsList .item .text .infor{font-size:24rpx;}
.shoppingCart .invalidGoods .goodsList .item .text .end{font-size:26rpx;color:#bbb;}
.shoppingCart .footer{width:100%;height:96rpx;background-color:#fafafa;position:fixed;bottom:0;padding:0 30rpx;box-sizing:border-box;border-top:1rpx solid #eee;left:0;}
.shoppingCart .footer .checkAll{font-size:28rpx;color:#101010;margin-left:16rpx;}
.shoppingCart .footer checkbox .wx-checkbox-input{background-color:#fafafa;}
.shoppingCart .footer .money{font-size:30rpx;}
.shoppingCart .footer .hj{font-size:24rpx;font-weight: bold;}
.shoppingCart .footer .amt{font-size:24rpx;font-weight: bold;}
.shoppingCart .footer .del{font-size:28rpx; color: white;width:120rpx;margin-left:30rpx;background-color: #FF7875;text-align: center; height:70rpx;border-radius: 35rpx; line-height: 70rpx;}
.shoppingCart .footer .placeOrder{color:#fff;font-size:30rpx;width:160rpx;height:70rpx;border-radius:50rpx;text-align:center;line-height:70rpx;margin-left:22rpx;}
.shoppingCart .footer .button .bnt{font-size:28rpx;color:#999;border-radius:50rpx;border:1px solid #999;width:160rpx;height:60rpx;text-align:center;line-height:60rpx;}
.shoppingCart .footer .button form~form{margin-left:17rpx;}

.shoppingCart .footer2{width:100%;height:96rpx;background-color:#fafafa;padding:0 30rpx;box-sizing:border-box;border-top:1rpx solid #eee;left:0;}
.shoppingCart .footer2 .checkAll{font-size:28rpx;color:#101010;margin-left:16rpx;}
.shoppingCart .footer2 checkbox .wx-checkbox-input{background-color:#fafafa;}
.shoppingCart .footer2 .money{font-size:30rpx;}
.shoppingCart .footer2 .hj{font-size:24rpx;font-weight: bold;}
.shoppingCart .footer2 .amt{font-size:24rpx;font-weight: bold;}
.shoppingCart .footer2 .del{font-size:28rpx; color: white;width:120rpx;margin-left:30rpx;background-color: #FF7875;text-align: center; height:70rpx;border-radius: 35rpx; line-height: 70rpx;}
.shoppingCart .footer2 .placeOrder{color:#fff;font-size:30rpx;width:160rpx;height:70rpx;border-radius:50rpx;text-align:center;line-height:70rpx;margin-left:22rpx;}
.shoppingCart .footer2 .button .bnt{font-size:28rpx;color:#999;border-radius:50rpx;border:1px solid #999;width:160rpx;height:60rpx;text-align:center;line-height:60rpx;}
.shoppingCart .footer2 .button form~form{margin-left:17rpx;}
#noBox {
  position: absolute;
  z-index: 99999;
  height: 40rpx;
  width: 40rpx;
  border-radius: 50%;
  background-color: rgba(5, 5, 5, 0.445);
}
#mas {
  position: fixed;
  z-index: 9999;
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  background-color: rgba(8, 8, 8, 0.384);
}
#masModel{
  position: fixed;
  z-index: 99999;
  top: 40%;
  left: 20%;
  right: 20%;
  bottom: 40%rpx;
  height: 300rpx;
  box-sizing: border-box;
  padding: 20rpx;
  background-color: #fff;
  border-radius: 20rpx;
  text-align: center;
}
#masModel .text {
  margin-top: 48rpx;
  margin-bottom: 60rpx;
}
#masModel .btn {
  padding: 6rpx 20rpx;
  background-color: #06B7A4;
  border-radius: 20rpx;
  color: #fff;
}

